{
  "LogLevel": {
    "Default": "Warning"
  },
  "Console": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Debug": {
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "EventLog": {
    "SourceName": "LoggerDemo"
    //MachineName: The local machine name is used.
  },
  "ApplicationInsights": {
    "LogLevel": {
      "Default": "Error",
      "ImportantClass": "Information"
    }
  },
  //The file logger uses the SeriLog implementation
  "Serilog": {
    "RetainedFileCountLimit": 7,
    "FileSizeLimitBytes": 20971520, // = 20MB
    "LogLevel": {
      "Default": "Trace"
    }
  }

}
